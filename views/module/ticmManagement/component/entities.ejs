<div class="content">
	<div class="card">
		<div class="card-header header-elements-inline">
			<h5 class="card-title"><%= module.ticmManagement.language[page.lang].entities_panel_tableTitle%></h5>
			<div class="header-elements">
				<div class="list-icons">
					<a class="list-icons-item" data-action="collapse"></a>
				</div>
			</div>
		</div>
		<div class="card-body">
			<%= module.ticmManagement.language[page.lang].entities_panel_tableDescription%>
		</div>
		<div id="dataTableContent">
			<table class="table datatable-basic table-striped">
				<thead>
					<tr>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnAction%></th>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnId%></th>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnImage%></th>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnName%></th>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnDescription%></th>
						<th><%= module.ticmManagement.language[page.lang].entities_panel_tableColumnStatus%></th>
					</tr>
				</thead>
				<tbody>
				</tbody>
			</table>
		</div>
	</div>
</div>


<div id="modalEditEntity" class="modal fade" tabindex="1">
	<div class="modal-dialog">
		<div class="modal-content">
			<div id="modalHeader" class="modal-header bg-primary">
				<h6 id="modalTitle" class="modal-title"></h6>
				<button type="button" class="close" data-dismiss="modal">&times;</button>
			</div>
			<div class="modal-body">
				<div
					class="alert alert-info alert-dismissible alert-styled-left border-top-0 border-bottom-0 border-right-0">
					<%= module.ticmManagement.language[page.lang].entities_panel_modal_warning%>
					<button type="button" class="close" data-dismiss="alert">Ã—</button>
				</div>
				<ul class="nav nav-tabs nav-tabs-solid nav-justified border-0">
					<li class="nav-item"><a href="#tabEntity" class="nav-link active" data-toggle="tab"><i
								class="icon-user mr-2"></i><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity%></a>
					</li>
					<li class="nav-item"><a href="#tabModules" class="nav-link" data-toggle="tab"><i
								class="icon-puzzle mr-2"></i><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_modules%></a>
					</li>
				</ul>

				<div class="tab-content">
					<div class="tab-pane fade show active" id="tabEntity">
						<div class="row">
							<div class="col-md">
								<label><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_name%></label>
								<div class="input-group">
									<span class="input-group-prepend">
										<span class="input-group-text"><i class="icon-user"></i></span>
									</span>
									<input id="inputEntityName" type="text" class="form-control" readonly
										placeholder="<%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_name_placeholder%>"
										value="">
								</div>
								<br>

								<label><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_phone%></label>
								<div class="input-group">
									<span class="input-group-prepend">
										<span class="input-group-text"><i class="icon-user"></i></span>
									</span>
									<input id="inputEntityPhone" type="text" class="form-control" readonly
										placeholder="<%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_phone_placeholder%>"
										value="">
								</div>
								<br>

								<label><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_news%></label>
								<div class="input-group">
									<span class="input-group-prepend">
										<span class="input-group-text"><i class="icon-user"></i></span>
									</span>
									<input id="inputEntityNews" type="text" class="form-control" readonly
										placeholder="<%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_news_placeholder%>"
										value="">
								</div>
								<br>

								<label><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_rss%></label>
								<div class="input-group">
									<span class="input-group-prepend">
										<span class="input-group-text"><i class="icon-user"></i></span>
									</span>
									<input id="inputEntityRss" type="text" class="form-control" readonly
										placeholder="<%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_rss_placeholder%>"
										value="">
								</div>
								<br>

								<label><%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_description%></label>
								<div class="input-group">
									<span class="input-group-prepend">
										<span class="input-group-text"><i class="icon-user"></i></span>
									</span>
									<input id="inputEntityDescription" type="text" class="form-control" readonly
										placeholder="<%= module.ticmManagement.language[page.lang].entities_panel_modal_tab_entity_label_description_placeholder%>"
										value="">
								</div>

							</div>
						</div>

					</div>

					<div class="tab-pane fade" id="tabModules">
						<select id="selectListBoxModules" multiple="multiple" class="form-control listbox-module"
							data-fouc></select>
					</div>
				</div>
			</div>
			<div class="modal-footer">
				<button type="button" data-dismiss="modal"
					class="btn bg-primary"><%= module.ticmManagement.language[page.lang].users_panel_modalEditUser_button_close%></button>
			</div>
		</div>
	</div>
</div>
<!-- /default modal -->